<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.newcart.pariscartrestaurant">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/paris_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/paris_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MotoristasActivity"></activity>
        <activity android:name=".InfoCryptoActivity" />
        <activity android:name=".OnBoardActivity" />
        <activity android:name=".PrintDialogActivity" />
        <activity android:name=".UpdateAddActivity" />
        <activity android:name=".ListAdicionaisActivity" />
        <activity android:name=".NewAddActivity" />
        <activity android:name=".UpdateFoodActivity" />
        <activity android:name=".CadastroActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".HelpActivity" />
        <activity android:name=".TermosActivity" />
        <activity android:name=".AddNewFood" />
        <activity android:name=".OrderDetailsActivity" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".Services.MyFCMServices">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>